{
  "expo": {
    "name": "meu-app-pesquisa-precos",
    "slug": "meu-app-pesquisa-precos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "meuapppesquisaprecos",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2E7D32"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-96x96.png",
      "description": "Sistema de coleta de pre√ßos de mercado",
      "themeColor": "#2E7D32",
      "pwa": {
        "display": "standalone",
        "orientation": "portrait",
        "themeColor": "#2E7D32",
        "backgroundColor": "#ffffff",
        "manifest": {
          "short_name": "Pesquisa",
          "start_url": "/"
        }
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
